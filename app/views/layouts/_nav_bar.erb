


<div class="button-container">
<% if !logged_in? %>

   <button class="sign-up-button"> <%= link_to "Sign Up", new_user_path %> </button>
   <button class="login-button"> <%= link_to "Log In",  login_path%> </button>

<% else %>

    <button class="logout-button"><%= link_to "Log Out", logout_path,  method: :delete %> </button>
    
<% end %>



