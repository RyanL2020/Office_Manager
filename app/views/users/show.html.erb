<%= stylesheet_link_tag 'usershow.css'%>

<h1> Welcome <%= @user.first_name %> </h1>   
        <ul>
        <li> First name: <%= @user.first_name %>   </li>
        <li> Last name: <%= @user.last_name  %>  </li>
        <li> Email address: <%= @user.email %>  </li>
        <li> Username: <%= @user.username %>  </li>
        </ul>

       

        <button><%= link_to "EDIT This User's Info", edit_user_path(@user)%></button> 
        <button><%= link_to "DELETE This User ", @user, method: :delete %> </button> 
       
      
        
        
        
        