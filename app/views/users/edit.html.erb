<%= form_with model: @user do |f| %>
    
    First Name: <%= f.text_field :first_name, placeholder: @user.first_name %><br><br>
    Last Name: <%= f.text_field :last_name, placeholder: @user.last_name %><br><br>
    Username: <%= f.text_field :username, placeholder: @user.username %><br><br>
    
    <%= f.submit "EDIT User" %>
    
<% end %>